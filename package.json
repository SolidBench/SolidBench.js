    "version": "manual-git-changelog onversion"
  },
  "dependencies": {
    "@solid/community-server": "^7.0.0",
    "@types/dockerode": "^3.0.0",
    "@types/unzipper": "^0.10.0",
    "dockerode": "^4.0.0",
    "ldbc-snb-enhancer": "^2.5.0",
    "ldbc-snb-validation-generator": "^1.1.0",
    "rdf-dataset-fragmenter": "^2.8.0",
