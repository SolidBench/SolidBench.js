{
  "name": "solidbench",
  "version": "1.7.1",
  "description": "A benchmark for Solid to simulate vaults with social network data.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest ${1}",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint . --ext .ts --cache",
    "prepare": "husky",
    "version": "manual-git-changelog onversion"
  },
  "keywords": [
    "solidbench",
    "solid",
    "benchmark",
    "decentralized",
    "ldbc",
    "snb",
    "social network"
  ],
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "repository": "git@github.com:SolidBench/SolidBench.js.git",
  "bugs": {
    "url": "https://github.com/SolidBench/SolidBench.js/issues"
  },
  "homepage": "https://github.com/SolidBench/SolidBench.js#readme",
  "license": "MIT",
  "bin": {
    "solidbench": "bin/solidbench"
  },
  "files": [
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "bin/**/*.js.map",
    "bin/solidbench",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.json",
    "templates/**/*",
    "lib/**/*.js.map",
    "index.d.ts",
    "index.js.map",
    "index.ts"
  ],
  "dependencies": {
    "@solid/community-server": "^7.0.0",
    "@types/dockerode": "^3.2.3",
    "@types/unzipper": "^0.10.5",
    "@types/yargs": "^16.0.1",
    "dockerode": "^3.2.1",
    "ldbc-snb-enhancer": "^2.5.1",
    "ldbc-snb-validation-generator": "^1.1.0",
    "rdf-dataset-fragmenter": "^2.7.0",
    "sparql-query-parameter-instantiator": "^2.5.1",
    "unzipper": "^0.10.11",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@rubensworks/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^7.9.0",
    "fs-extra": "^9.0.0",
    "husky": "^9.0.0",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.2",
    "manual-git-changelog": "^1.0.1",
    "rimraf": "^5.0.1",
    "ts-jest": "^26.4.3",
    "typescript": "^4.6.4"
  }
}
