    "yargs": "^17.0.0"
  },
  "devDependencies": {
    "@rubensworks/eslint-config": "^3.0.0",
    "@types/jest": "^29.0.0",
    "@types/yargs": "^17.0.0",
    "eslint": "^8.0.0",
    "husky": "^9.0.0",
    "jest": "^29.0.0",
